{
  "name": "feathers-scraper",
  "version": "0.0.0-semantic-release",
  "description": "A metadata, html, and text scraper proxy for FeathersJS",
  "main": "lib",
  "scripts": {
    "lint": "semistandard --fix",
    "mocha": "printf '\\33c\\e[3J' && mocha --recursive test/**/*.test.js",
    "testd": "docker-compose up -d && npm run test && docker-compose down",
    "test": "printf '\\33c\\e[3J' && npm run lint && npm run coverage",
    "test:dev": "printf '\\33c\\e[3J' && npm run lint && npm run mocha -- --watch --parallel --watch-files ./test/**/*,./lib/**",
    "test:two": "npm run mocha -- --watch --parallel --watch-files ./test/**/*,./lib/**",
    "coverage": "nyc npm run mocha"
  },
  "old_scripts": {
    "OLD_test:dev": "npm run mocha -- --watch --parallel --watch-files src/**/*.js, test/**/*.js test/**/*.test.js",
    "OLD_test": "mocha ./test/",
    "parse": "node parse.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": ">= 14.20.0"
  },
  "semistandard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "/src/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesvillarrubia/feathers-scraper.git"
  },
  "keywords": [
    "feathers",
    "crawler",
    "feathers-plugin",
    "scrape",
    "spider",
    "html",
    "metadata"
  ],
  "author": {
    "name": "James Villarrubia",
    "email": "james.villarrubia@gmail.com"
  },
  "license": "MIT",
  "node": ">= 12",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jamesvillarrubia/feathers-scraper/issues"
  },
  "homepage": "https://github.com/jamesvillarrubia/feathers-scraper#readme",
  "dependencies": {
    "@feathersjs/commons": "^4.5.12",
    "@feathersjs/errors": "^4.5.12",
    "browserless": "^9.6.6",
    "crawler": "^1.3.0",
    "csv-parse": "^5.3.0",
    "headless-chrome-crawler": "^1.8.0",
    "html-get": "^2.9.22",
    "html-to-text": "^8.2.1",
    "metascraper": "^5.30.3",
    "metascraper-audio": "^5.30.3",
    "metascraper-author": "^5.30.3",
    "metascraper-clearbit": "^5.30.3",
    "metascraper-date": "^5.30.3",
    "metascraper-description": "^5.30.3",
    "metascraper-iframe": "^5.30.3",
    "metascraper-image": "^5.30.3",
    "metascraper-lang": "^5.30.3",
    "metascraper-logo": "^5.30.3",
    "metascraper-logo-favicon": "^5.30.3",
    "metascraper-publisher": "^5.30.3",
    "metascraper-readability": "^5.30.3",
    "metascraper-title": "^5.30.3",
    "metascraper-url": "^5.30.3",
    "metascraper-video": "^5.30.3",
    "node-fetch": "^3.2.10",
    "puppeteer": "^16.2.0",
    "robots-parser": "^3.0.0"
  },
  "devDependencies": {
    "@feathersjs/feathers": "^4.5.12",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "assert": "^2.0.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "rewire": "^6.0.0",
    "eslint": "^8.10.0",
    "nyc": "^15.1.0",
    "semistandard": "^16.0.1"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "alpha",
        "channel": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/git",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ]
    ]
  }
}
