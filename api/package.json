{
  "name": "api",
  "description": "FeathersJS based API for NASA's MADI infrastructure",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.8.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "src/client",
  "type": "module",
  "scripts": {
    "start": "node src",
    "dev": "nodemon src/",
    "prettier": "npx prettier \"**/*.js\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "bundle:client": "npm pack --pack-destination ./public",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make"
  },
  "dependencies": {
    "@extractus/article-extractor": "^7.2.16",
    "@feathersjs/adapter-commons": "^5.0.3",
    "@feathersjs/authentication": "^5.0.3",
    "@feathersjs/authentication-client": "^5.0.3",
    "@feathersjs/authentication-local": "^5.0.6",
    "@feathersjs/authentication-oauth": "^5.0.6",
    "@feathersjs/configuration": "^5.0.3",
    "@feathersjs/errors": "^5.0.3",
    "@feathersjs/feathers": "^5.0.3",
    "@feathersjs/knex": "^5.0.3",
    "@feathersjs/koa": "^5.0.3",
    "@feathersjs/schema": "^5.0.3",
    "@feathersjs/socketio": "^5.0.3",
    "@feathersjs/transport-commons": "^5.0.3",
    "@feathersjs/typebox": "^5.0.3",
    "ansi-escapes": "^6.2.0",
    "ansi-to-html": "^0.7.2",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "d3-dsv": "^2.0.0",
    "deepmerge": "^4.3.1",
    "dot-prop": "^8.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fast-levenshtein": "^3.0.0",
    "feathers-blob": "^2.6.0",
    "figures": "^5.0.0",
    "html-to-text": "^9.0.5",
    "input-event": "^1.2.6",
    "inquirer": "^9.2.6",
    "js-tiktoken": "^1.0.6",
    "knex": "^2.4.2",
    "langchain": "^0.0.96",
    "multer": "^1.4.5-lts.1",
    "normalize-url": "^8.0.0",
    "openai": "^3.3.0",
    "pg": "^8.10.0",
    "playwright": "^1.34.3",
    "redis": "^4.6.6",
    "strip-ansi": "^7.1.0",
    "tiktoken": "^1.0.7",
    "typeorm": "^0.3.16",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.3",
    "@feathersjs/rest-client": "^5.0.3",
    "@inquirer/core": "^2.1.0",
    "@inquirer/prompts": "^2.1.1",
    "@inquirer/type": "^1.1.0",
    "0x": "^5.5.0",
    "axios": "^1.3.5",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "fs-blob-store": "^6.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.7",
    "stack-trace": "^1.0.0-pre2"
  }
}
