{
  "name": "observations",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "James Villarrubia",
    "email": "james.villarrubia@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 16.19.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "postgresql",
    "framework": "koa",
    "transports": [
      "rest"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "type": "module",
  "main": "src/index",
  "scripts": {
    "start": "node src",
    "dev": "nodemon src/",
    "prettier": "npx prettier \"**/*.js\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "bundle:client": "npm pack --pack-destination ./public",
    "spec:build": "swagger-merger -i ./specifications/openapi/spec.yml -o ./specifications/openapi/_merged.yml",
    "spec:validate": "npx swagger-cli validate ./specifications/openapi/_merged.yml",
    "spec:mock": "npx @stoplight/prism-cli mock ./specifications/openapi/_merged.yml",
    "spec:swagger": "node ./node_modules/swagger-to-static/index.js ./specifications/openapi/_merged.yml ./specifications/build",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.0",
    "@feathersjs/authentication": "^5.0.0",
    "@feathersjs/authentication-client": "^5.0.0",
    "@feathersjs/configuration": "^5.0.0",
    "@feathersjs/errors": "^5.0.0",
    "@feathersjs/feathers": "^5.0.0",
    "@feathersjs/knex": "^5.0.0",
    "@feathersjs/koa": "^5.0.0",
    "@feathersjs/schema": "^5.0.0",
    "@feathersjs/socketio": "^5.0.0",
    "@feathersjs/transport-commons": "^5.0.0",
    "@feathersjs/typebox": "^5.0.0",
    "@koa/multer": "^3.0.2",
    "@koa/router": "^12.0.0",
    "compression": "^1.7.4",
    "feathers-openai": "^0.0.0-semantic-release",
    "feathers-rpc": "^1.0.1",
    "json2md": "^2.0.0",
    "knex": "^2.4.2",
    "koa-mount": "^4.0.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^12.0.0",
    "multer": "^1.4.5-lts.1",
    "mural-db": "^1.0.2",
    "openai": "^3.1.0",
    "pg": "^8.9.0",
    "showdown": "^2.1.0",
    "wikipedia": "^2.0.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.0",
    "@feathersjs/rest-client": "^5.0.0",
    "axios": "^1.3.5",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "swagger-merger": "^1.5.4",
    "swagger-to-static": "^1.1.0"
  }
}
