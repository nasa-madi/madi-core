{
  "name": "obs2",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 16.19.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "other",
    "framework": "express",
    "transports": [
      "rest"
    ],
    "schema": false
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "lib/index",
  "type": "module",
  "scripts": {
    "start": "node src",
    "dev": "nodemon src/",
    "prettier": "npx prettier \"**/*.js\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "npm run mocha",
    "bundle:client": "npm pack --pack-destination ./public",
    "spec:build": "npx swagger-merger -i ./specifications/openapi/spec.yml -o ./specifications/openapi/_merged.yml",
    "spec:validate": "npx swagger-cli validate ./specifications/openapi/_merged.yml",
    "spec:mock": "npx @stoplight/prism-cli mock ./specifications/openapi/_merged.yml",
    "spec:swagger": "node ./node_modules/swagger-to-static/index.js ./specifications/openapi/_merged.yml ./specifications/build"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.0-pre.38",
    "@feathersjs/authentication": "^5.0.0-pre.38",
    "@feathersjs/authentication-client": "^5.0.0-pre.38",
    "@feathersjs/configuration": "^5.0.0-pre.38",
    "@feathersjs/errors": "^5.0.0-pre.38",
    "@feathersjs/express": "^5.0.0-pre.38",
    "@feathersjs/feathers": "^5.0.0-pre.38",
    "@feathersjs/schema": "^5.0.0-pre.38",
    "@feathersjs/transport-commons": "^5.0.0-pre.38",
    "compression": "^1.7.4",
    "feathers-rpc": "^1.0.1",
    "json2md": "^2.0.0",
    "mural-db": "^1.0.2",
    "showdown": "^2.1.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.0-pre.38",
    "@feathersjs/rest-client": "^5.0.0-pre.38",
    "axios": "^1.3.3",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "swagger-to-static": "^1.1.0"
  }
}
