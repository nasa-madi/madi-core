    "@feathersjs/express": "^5.0.0-pre.37",
    "@feathersjs/feathers": "^5.0.0-pre.37",
    "@feathersjs/schema": "^5.0.0-pre.37",
    "@feathersjs/transport-commons": "^5.0.0-pre.37",
    "@feathersjs/typebox": "^5.0.0-pre.37",
    "compression": "^1.7.4",
    "json2md": "^2.0.0",
